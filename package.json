{
  "name": "spotify-mini",
  "version": "2.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "type": "module",
  "author": {
    "email": "rocktimthedev@gmail.com",
    "name": "Rocktim Saikia",
    "url": "https://rocktimcodes.site"
  },
  "funding": "https://buymeacoffee.com/rocktimcodes",
  "repository": "rocktimsaikia/spotify-mini",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsup source/index.ts --dts --format esm",
    "pretest": "npm run build",
    "test": "ava --timeout 10s",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "ava": "^4.2.0",
    "dotenv": "^16.0.0",
    "husky": "^7.0.4",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "spotify-types": "^1.0.7",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "node-fetch": "^3.2.3"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "np": {
    "contents": "dist"
  },
  "keywords": [
    "spotify",
    "spotify-api",
    "spotify-typescript",
    "spotify-current-song",
    "spotify-last-played-song"
  ],
  "lint-staged": {
    "*.{js,ts,md}": "prettier --write"
  }
}
